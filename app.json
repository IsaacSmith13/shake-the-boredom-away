{
  "expo": {
    "name": "shakeTheBoredomAway",
    "slug": "shakeTheBoredomAway",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "shake-the-boredom-away"
    },
    "android": {
      "package": "shake-the-boredom-away"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
